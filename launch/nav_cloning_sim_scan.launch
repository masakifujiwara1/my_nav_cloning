<?xml version="1.0"?>
<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!-- <arg name="world_name" value="$(find nav_cloning)/world/willow_garage.world"/>  -->
    <!-- <arg name="world_name" value="$(find nav_cloning)/world/cross_road.world"/>  -->
    <!-- <arg name="world_name" value="$(find nav_cloning)/world/Tsudanuma_2-3.world"/>  -->
    <arg name="world_name" value="$(find nav_cloning)/world/tsudanuma2-3.world"/> 

    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_waffle_pi.urdf.xacro" />

  <!-- <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model mobile_base -x -10.0 -y 29.0 -z 0.3 -param robot_description" /> -->
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model mobile_base -x -10.0 -y 29.0 -z 0.3 -param robot_description" />

     <node pkg="timed_roslaunch" type="timed_roslaunch.sh"
        args="10 nav_cloning nav_cloning.launch"
        name="timed_roslaunch" output="screen" />

  <!-- <node pkg="timed_roslaunch" type="timed_roslaunch.sh"
        args="8 nav_cloning turtlebot3_navigation.launch model:=waffle map_file:=$(find nav_cloning)/maps/cross_road.yaml waypoints_file:=$(find nav_cloning)/maps/cross_road_way_cmd.yaml"
        name="timed_roslaunch2" > -->
      <node pkg="timed_roslaunch" type="timed_roslaunch.sh"
        args="10 nav_cloning turtlebot3_navigation.launch model:=waffle map_file:=$(find nav_cloning)/maps/mymap_for_costmap.yaml waypoints_file:=$(find nav_cloning)/maps/cit3f_way_fix004.yaml"
        name="timed_roslaunch2" >
  </node>
</launch>
