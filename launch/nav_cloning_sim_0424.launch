<?xml version="1.0"?>
<launch>
  <arg name="script" default="nav_cloning_node.py"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find icart_mini_gazebo)/worlds/Tsudanuma_2-3.world"/> 
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <param name="robot_description" command="$(find xacro)/xacro $(find turtlebot3_description)/urdf/turtlebot3_waffle_pi.urdf.xacro" />

  <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model mobile_base -x 0.0 -y 0.0 -z 0.0 -param robot_description" />

  <node pkg="timed_roslaunch" type="timed_roslaunch.sh"
        args="5 nav_cloning nav_cloning.launch script:=$(arg script)"
        name="timed_roslaunch" output="screen" />

  <node pkg="timed_roslaunch" type="timed_roslaunch.sh"
	  args="8 nav_cloning turtlebot3_navigation_tsudanuma2-3.launch model:=waffle map_file:=/home/fmasa/Downloads/tsudanuma0204.yaml waypoints_file:=$(find turtlebot3_gazebo)/worlds/tsudanuma2-3/tsudanuma2-3_wp.yaml"
        name="timed_roslaunch2" >
  </node>
<!--
  <node pkg="timed_roslaunch" type="timed_roslaunch.sh"
        args="20 nav_cloning start_wp_nav.launch"
        name="timed_roslaunch3" >
  </node>
-->
</launch>
